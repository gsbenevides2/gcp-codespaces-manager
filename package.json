{
	"name": "gcp-codespaces-manager",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "bun src/server.ts",
		"dev": "bun --watch src/server.ts",
		"lint": "bunx --bun biome check --write",
		"swagger:build": "cd node_modules/@elysiajs/swagger && bun install && bun run build || true && rm -rf node_modules && cd ../../..",
		"prepare": "husky"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.3.3",
		"@elysiajs/server-timing": "^1.3.0",
		"@elysiajs/swagger": "github:gsbenevides2/elysia-swagger",
		"@grotto/logysia": "^0.1.6",
		"elysia": "latest"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@commitlint/config-conventional": "^19.8.1",
		"bun-types": "latest",
		"husky": "^9.1.7"
	}
}
